result-view{
  match: MoviesStructure (movies) {
    from-output: DailyBoxOffice(action)
  }

  render{
    if (size(movies) > 1){
      list-of (movies){
        where-each (movie){
          layout-macro (movie-summary) {
            param (movie){
              expression (movie)
            }
          }
        }
      }      
    }
    else-if (size(movies) == 1){
      layout-macro (movie-details){
        param (movie){
          expression (movies)
        }
      }  
    }
  }
}