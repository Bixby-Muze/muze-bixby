action (MovieList) {
  type(Search)
  description (__DESCRIPTION__)
  collect {
    input (_movieNm) {
      type (MovieNm)
      min (Optional) max (One)
    }
    
    input (_directorNm) {
      type (DirectorNm)
      min (Optional) max (One)
    }

    input (_openStartDt) {
      type (OpenStartDt)
      min (Optional) max (One)
    }

    input (_openEndDt) {
      type (OpenEndDt)
      min (Optional) max (One)
    }
  }
  output (MovieListStructure)
}
